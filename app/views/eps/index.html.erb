<p id="notice"><%= notice %></p>

<h1>Eps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eps.each do |ep| %>
      <tr>
        <td><%= ep.name %></td>
        <td><%= ep.active %></td>
        <td><%= link_to 'Show', ep %></td>
        <td><%= link_to 'Edit', edit_ep_path(ep) %></td>
        <td><%= link_to 'Destroy', ep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ep', new_ep_path %>
